@model Instructor

@{
    ViewData["Title"] = "Details - Instructors";
}
<!-- Strong Type View -->
<!-- List Department -->

<div>
    <h1>Name: @Model.Name</h1>
    <h2>Dept: @Model.DepartmentID</h2>
    <h3>Address: @Model.Address</h3>
    <h4>Salary: @Model.Salary$</h4>
</div>
<a asp-action="Index" asp-controller="Instructor" asp-fragment="container" class="btn btn-primary mt-3"> Back To All Instructors </a>